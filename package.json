{
  "name": "kiel-react-ts",
  "version": "0.0.1",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node server.js",
    "build": "./node_modules/.bin/webpack",
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "node_modules/typescript-babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/components",
      "src/containers",
      "src/models",
      "src/reducers",
      "src/sagas",
      "src/store"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "16.0.0",
    "@types/marked": "0.0.28",
    "@types/material-ui": "0.16.41",
    "@types/react": "0.14.50",
    "@types/react-dom": "0.14.19",
    "@types/react-hot-loader": "3.0.0",
    "@types/react-redux": "4.4.33",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "3.6.31",
    "@types/redux-actions": "0.8.33",
    "@types/redux-devtools": "3.0.35",
    "@types/redux-saga": "0.9.30",
    "@types/webpack": "2.0.0",
    "awesome-typescript-loader": "2.2.4",
    "babel": "6.5.2",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-regenerator": "6.16.1",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "express": "4.14.0",
    "jest": "17.0.3",
    "marked": "0.3.6",
    "material-ui": "0.16.4",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "4.4.6",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.4.1",
    "redux": "3.6.0",
    "redux-actions": "1.0.1",
    "redux-devtools": "3.3.1",
    "redux-saga": "0.13.0",
    "source-map-loader": "0.1.5",
    "ts-loader": "1.2.2",
    "typescript": "2.0.10",
    "typescript-babel-jest": "0.1.8",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  }
}