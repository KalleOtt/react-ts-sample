{
  "name": "kiel-react-ts",
  "version": "0.0.1",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node server.js",
    "build": "webpack",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KalleOtt/react-ts-sample.git"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "node_modules/typescript-babel-jest",
      "^.+\\.(js|jsx)$": "node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "src/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/*.{ts,tsx}",
      "src/actions/*.{ts,tsx}",
      "src/components/*.{ts,tsx}",
      "src/containers/*.{ts,tsx}",
      "src/models/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/redux/*.{ts,tsx}",
      "src/store/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/actions",
      "src/components",
      "src/containers",
      "src/models",
      "src/reducers",
      "src/redux",
      "src/store"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "src/__tests__/.*\\.test\\.(ts|tsx|js|jsx)$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "16.0.0",
    "@types/marked": "0.0.28",
    "@types/react": "0.14.50",
    "@types/react-dom": "0.14.19",
    "@types/react-hot-loader": "3.0.0",
    "@types/react-redux": "4.4.33",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "3.6.31",
    "@types/redux-actions": "0.8.33",
    "@types/redux-devtools": "3.0.35",
    "@types/redux-mock-store": "0.0.7",
    "@types/redux-saga": "0.9.30",
    "@types/webpack": "2.0.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "2.2.4",
    "babel": "6.5.2",
    "babel-core": "6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-regenerator": "6.16.1",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "^0.26.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^2.0.0",
    "jest": "17.0.3",
    "marked": "0.3.6",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.1",
    "react": "15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "4.4.6",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.4.1",
    "react-toolbox": "^1.2.5",
    "redux": "3.6.0",
    "redux-actions": "1.0.1",
    "redux-devtools": "3.3.1",
    "redux-mock-store": "^1.2.1",
    "redux-observable": "^0.12.2",
    "redux-saga": "0.13.0",
    "rxjs": "^5.0.0-rc.4",
    "sass-loader": "^4.0.2",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "1.2.2",
    "typescript": "2.0.10",
    "typescript-babel-jest": "0.1.8",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  }
}
