{
  "name": "kiel-react-ts",
  "version": "0.1.0",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node hotReloadingServer.js",
    "build": "webpack",
    "test": "jest --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KalleOtt/react-ts-sample.git"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "src/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/components/*.{ts,tsx}",
      "src/containers/*.{ts,tsx}",
      "src/models/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/redux/*.{ts,tsx}",
      "src/store/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/actions",
      "src/components",
      "src/containers",
      "src/models",
      "src/reducers",
      "src/redux",
      "src/store"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "src/__tests__/.*\\.test\\.(ts|tsx|js|jsx)$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/marked": "^0.0.28",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.38",
    "@types/redux-actions": "^1.2.4",
    "@types/redux-devtools": "^3.0.37",
    "@types/webpack": "^2.2.15",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^2.1.2",
    "jest": "^19.0.2",
    "marked": "^0.3.6",
    "node-sass": "^4.5.2",
    "normalize.css": "^6.0.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-modules-values": "^1.2.2",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^15.5.4",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.3",
    "redux-observable": "^0.14.1",
    "redux-saga": "^0.14.6",
    "rxjs": "^5.3.0",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "ts-jest": "^19.0.10",
    "typescript": "^2.2.2",
    "typescript-babel-jest": "^1.0.3",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
