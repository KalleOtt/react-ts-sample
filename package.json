{
  "name": "kiel-react-ts",
  "version": "0.1.0",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node hotReloadingServer.js",
    "build": "webpack",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KalleOtt/react-ts-sample.git"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "src/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/components/*.{ts,tsx}",
      "src/containers/*.{ts,tsx}",
      "src/models/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/redux/*.{ts,tsx}",
      "src/store/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/actions",
      "src/components",
      "src/containers",
      "src/models",
      "src/reducers",
      "src/redux",
      "src/store"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "notify": true,
    "testRegex": "src/__tests__/.*\\.test\\.(ts|tsx|js|jsx)$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^16.0.4",
    "@types/marked": "^0.0.28",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-redux": "^4.4.35",
    "@types/redux-actions": "^1.2.2",
    "@types/redux-devtools": "^3.0.36",
    "@types/webpack": "^2.1.0",
    "autoprefixer": "^6.6.1",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-regenerator": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^2.1.1",
    "jest": "^18.1.0",
    "marked": "^0.3.6",
    "node-sass": "^4.3.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-test-renderer": "^15.4.2",
    "react-toolbox": "1.2.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.1",
    "redux-observable": "^0.12.2",
    "redux-saga": "^0.14.2",
    "rxjs": "^5.0.3",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5",
    "typescript-babel-jest": "^0.1.8",
    "webpack": "2.2.0-rc.4",
    "webpack-dev-server": "^2.2.0-rc"
  }
}
