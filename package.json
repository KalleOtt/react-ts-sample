{
  "name": "kiel-react-ts",
  "version": "0.1.0",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node hotReloadingServer.js",
    "build": "webpack",
    "test": "jest --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KalleOtt/react-ts-sample.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.test\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "src/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    },
    "snapshotSerializers": ["jest-glamor-react"],
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/components/*.{ts,tsx}",
      "src/containers/*.{ts,tsx}",
      "src/models/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/redux/*.{ts,tsx}",
      "src/store/*.{ts,tsx}"
    ],
    "moduleDirectories": ["node_modules", "src"]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "@types/marked": "^0.3.0",
    "@types/react": "^16.0.1",
    "@types/react-dom": "^15.5.2",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-redux": "^4.4.47",
    "@types/redux-actions": "^1.2.7",
    "@types/redux-devtools": "^3.0.38",
    "@types/redux-mock-store": "^0.0.9",
    "@types/webpack": "^3.0.6",
    "awesome-typescript-loader": "^3.2.2",
    "express": "^4.15.4",
    "glamor": "^2.20.39",
    "glamorous": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^2.3.0",
    "jest": "^20.0.4",
    "jest-glamor-react": "^3.1.0",
    "marked": "^0.3.6",
    "react": "^16.0.0-beta.5",
    "react-dom": "^16.0.0-beta.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0-beta.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.2.3",
    "redux-observable": "^0.14.1",
    "redux-saga": "^0.15.6",
    "rxjs": "^5.4.2",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.10",
    "typescript": "^2.4.2",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "rgba-string": "^1.0.7"
  }
}
