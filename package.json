{
  "name": "kiel-react-ts",
  "version": "0.0.1",
  "description": "Small sample to show usage of react with typescript",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "node_modules/typescript-babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/components",
      "src/containers",
      "src/models",
      "src/reducers",
      "src/sagas",
      "src/store"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^16.0.0",
    "@types/marked": "0.0.28",
    "@types/material-ui": "^0.16.40",
    "@types/react": "^0.14.42",
    "@types/react-dom": "^0.14.18",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-redux": "^4.4.32",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^0.8.30",
    "@types/redux-devtools": "^3.0.34",
    "@types/redux-saga": "^0.9.30",
    "@types/webpack": "^2.0.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "express": "^4.14.0",
    "jest": "^17.0.3",
    "marked": "^0.3.6",
    "material-ui": "^0.16.4",
    "path": "^0.12.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
