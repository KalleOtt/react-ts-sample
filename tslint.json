{
  "rules": {
    "class-name": true,
    "comment-format": [true, "check-space"],
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-var-keyword": true,
    "triple-equals": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "prefer-function-over-method": true
  },
  "jsRules": {
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-trailing-whitespace": true,
    "triple-equals": true,
    "variable-name": [true, "ban-keywords"]
  }
}
